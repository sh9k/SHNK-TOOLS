<Window x:Class="SHNK.Tools.App.ResetGuestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Reset Guest"
        Width="250" Height="250"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        WindowStyle="ToolWindow"
        Background="#111111">

    <Grid Margin="16,-5,16,37">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="16"/>
            <RowDefinition/>
        </Grid.RowDefinitions>



        <UniformGrid Columns="4" Rows="1" Margin="-2,3,-2,3" Grid.Row="1" Grid.RowSpan="2">
            <Button Content="GL" Margin="6,63,6,87" Click="Gl_Click"/>
            <Button Content="KR" Margin="3,63,12,87" Click="Kr_Click"/>
            <Button Content="VNG" Margin="-2,63,8,87" Click="Vng_Click"/>
            <Button Content="TW" Margin="2,63,4,87" Click="Tw_Click"/>
        </UniformGrid>
    </Grid>
</Window>
